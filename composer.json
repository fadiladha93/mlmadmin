{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-curl": "*",
        "ext-json": "*",
        "authy/php": "^3.0",
        "aws/aws-sdk-php": "^3.87",
        "barryvdh/laravel-cors": "^0.11.4",
        "bitpay/php-client": "~4",
        "bogardo/mailgun": "^5.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^5.0",
        "guzzlehttp/guzzle": "^6.3",
        "imtigger/laravel-job-status": "^0.2.2",
        "jenkins-khan/jenkins-api": "dev-master",
        "kalnoy/nestedset": "4.3.*",
        "kordy/ticketit": "0.*",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mpdf/mpdf": "^8.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-credit-card-validator": "^0.0.6",
        "sentry/sentry-laravel": "^1.3",
        "twilio/sdk": "^5.30",
        "yajra/laravel-datatables-oracle": "^8.13",
        "zvook/php-skrill-quick-checkout": "1.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "xethron/migrations-generator": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/helpers"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
